                                                                    <div>
                                                                        <strong tal:content="membre/prenom" tal:condition="membre/prenom"></strong>
                                                                        <strong tal:content="membre/title" class="text-uppercase"></strong>
                                                                        <a tal:condition="membre/lien_vers_structure" tal:attributes="href membre/lien_vers_structure; title string:Se rendre sur la page de l'organisme; class string:structureMembre">
                                                                            <abbr tal:condition="membre/abreviation_structure" tal:attributes="title membre/structure" tal:content="membre/abreviation_structure" />
                                                                            <tal:block tal:condition="not:membre/abreviation_structure" tal:content="membre/structure" /> 
                                                                        </a>
                                                                        <tal:block tal:condition="not:membre/lien_vers_structure"> 
                                                                            <tal:block tal:condition="membre/abreviation_structure"> 
                                                                                <abbr class="structureMembre" tal:attributes="title membre/structure" tal:content="membre/abreviation_structure" />
                                                                            </tal:block>
                                                                            <p tal:condition="not:membre/abreviation_structure" tal:content="membre/structure" />
                                                                        </tal:block>
                                                                        <span tal:content="membre/metier"></span>
                                                                        <a tal:attributes="href string:mailto: ${membre/email}; title string:Lui &eacute;crire un email"><i class="fa fa-envelope fa-2x"></i></a>
                                                                    </div>    
                                                                </figcaption>
                                                            </figure>
                                                        </li>   
                                                    </tal:block>
                                                </ul>
                                            </div>
                                            <div class="footCol"></div>
                                        </div>    
                                    </div>

                                    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4 col-md-height col-lg-height col-full-height col3 equipe">
